{
  "name": "hebo-snapshot-repository-inmemory",
  "description": "Snapshot Repository implementation for hebo-js that stores everything in memory",
  "version": "1.4.1",
  "author": "Steve Caldwell <scaldwell@gmail.com> ()",
  "ava": {
    "failFast": true,
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/stevecaldwell77/hebo-snapshot-repository-inmemory/issues",
    "email": "scaldwell@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Steve Caldwell <scaldwell@gmail.com> ()"
  ],
  "dependencies": {
    "auto-bind": "^1.2.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "ava": "^0.25.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.5.1",
    "eslint-config-archereducation": "^4.3.1",
    "fixpack": "^2.3.1",
    "hebo-validation": "^5.0.0",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "nyc": "^13.0.1",
    "prettier": "^1.18.2",
    "prettier-config-archereducation": "^1.0.0",
    "remark-cli": "^5.0.0",
    "remark-preset-github": "^0.0.9",
    "shortid": "^2.2.13"
  },
  "engines": {
    "node": ">=8.16.1"
  },
  "homepage": "https://github.com/stevecaldwell77/hebo-snapshot-repository-inmemory",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "cqrs",
    "es",
    "hebo-js"
  ],
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "git add"
      ],
      "*.md": [
        "remark . -qfo",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    }
  },
  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "peerDependencies": {
    "hebo": ">=6.0.0"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevecaldwell77/hebo-snapshot-repository-inmemory"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "remark . -qfo && eslint .",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run lint && npm run ava",
    "test-coverage": "npm run lint && npm run nyc"
  }
}
